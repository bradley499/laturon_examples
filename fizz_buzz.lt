function fizzbuzz(max) {
    num = 1
    while (num <= max) {
        output = ""
        if (num % 3 == 0) {
            output = "Fizz"
        }
        if (num % 5 == 0) {
            output = output + "Buzz"
        }
        if (output == "") {
            print(num)
        } else {
            print(output)
        }
        num = num + 1
    }
}

fizzbuzz(100)
